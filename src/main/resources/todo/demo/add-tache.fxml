<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="todo.demo.AddTacheController">
    <TextField fx:id="titreField" promptText="Titre"/>
    <TextField fx:id="descriptionField" promptText="Description"/>
    <TextField fx:id="dateField" promptText="Date"/>
    <Button text="Ajouter" onAction="#handleAjouter"/>
    <Button text="Retour" onAction="#handleRetour"/>
</VBox>